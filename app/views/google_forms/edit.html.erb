<% content_for :title, "Editing google form" %>

<div class="md:w-2/3 w-full">
  <h1 class="font-bold text-4xl">Editing google form</h1>

  <%= render "form", google_form: @google_form %>

  <%= link_to "Show this google form", @google_form, class: "w-full sm:w-auto text-center mt-2 sm:mt-0 sm:ml-2 rounded-md px-3.5 py-2.5 bg-gray-100 hover:bg-gray-50 inline-block font-medium" %>
  <%= link_to "Back to google forms", google_forms_path, class: "w-full sm:w-auto text-center mt-2 sm:mt-0 sm:ml-2 rounded-md px-3.5 py-2.5 bg-gray-100 hover:bg-gray-50 inline-block font-medium" %>
</div>
