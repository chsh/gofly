<div id="<%= dom_id student %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">ID:</strong>
    <%= student.id %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Email:</strong>
    <%= student.email %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= student.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name reading:</strong>
    <%= student.name_reading %>
  </div>
</div>
